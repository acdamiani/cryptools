{
  "name": "typescript-nextjs-starter",
  "description": "A TypeScript starter for Next.js that includes all you need to build amazing projects",
  "version": "1.0.0",
  "private": true,
  "author": "Jo√£o Pedro Schmitz <hey@joaopedro.dev> (@jpedroschmitz)",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "starter",
    "typescript"
  ],
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "lint-fix": "eslint --ignore-path .gitignore --fix \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "analyze": "cross-env ANALYZE=true next build",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register 'test/**/*.spec.ts'"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "eslint --ignore-path .gitignore --fix",
      "prettier --ignore-path .gitignore --write"
    ]
  },
  "dependencies": {
    "@next/font": "13.0.6",
    "@primer/octicons-react": "17.9.0",
    "better-react-mathjax": "2.0.2",
    "big-integer": "1.6.51",
    "classnames": "2.3.2",
    "copy-to-clipboard": "3.3.3",
    "detect-browser": "5.3.0",
    "file-saver": "2.0.5",
    "next": "13.0.6",
    "normalize.css": "8.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.3.0",
    "@commitlint/config-conventional": "17.3.0",
    "@next/bundle-analyzer": "13.0.6",
    "@svgr/webpack": "6.5.1",
    "@types/file-saver": "2.0.5",
    "@types/mocha": "10.0.1",
    "@types/node": "18.11.12",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@typescript-eslint/eslint-plugin": "5.46.0",
    "@typescript-eslint/parser": "5.46.0",
    "assert": "2.0.0",
    "cheerio": "1.0.0-rc.12",
    "cross-env": "7.0.3",
    "eslint": "8.29.0",
    "eslint-config-next": "13.0.6",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.2",
    "lint-staged": "13.1.0",
    "mocha": "10.1.0",
    "prettier": "2.8.1",
    "shiki": "0.11.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.1",
    "typescript": "4.9.4"
  }
}
